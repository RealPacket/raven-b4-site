{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strict": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext",
      "ESNext.Array",
      "ESNext.Symbol"
    ],
    "baseUrl": "./src",
    "paths": {
      "@components/*": [
        "./components/*"
      ],
      "@ClickGUI/*": [
        "./components/ClickGUI/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
    }
  },
  "include": [
    "src/**/*"
  ]
}